# Supabase credentials (from your Supabase project settings → API)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-public-key

# Service role key (⚠️ DO NOT expose this to the browser — only use in server code)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Optional: JWT secret (if you need custom auth policies)
SUPABASE_JWT_SECRET=your-jwt-secret

# Next.js (optional custom settings)
NODE_ENV=development
